<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>


	<table name="userinfo" catalog="smartcampus" class="UserInfo">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="role" catalog="smartcampus" class="Role">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="users" catalog="smartcampus" class="Users">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="sys_users_roles" catalog="smartcampus"></table>
</hibernate-reverse-engineering>